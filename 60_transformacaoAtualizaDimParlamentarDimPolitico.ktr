    <sql>SELECT 
    pa.parlamentar_id as politico_id,
    pa.nome,
    pa.nome_urna,
    pa.foto,
    pa.data_nascimento,
    pa.idade,
    pa.cidade_natal,
    pa.estado_natal,
    pa.partido,
    pa.numero,
    pa.cargo,
    pa.grau_instrucao,
    pa.ocupacao,
    pa.genero,
    pa.cor_raca,
    pa.url,
    pa.email,
    pa.historico_partidos,
    pa.excelencia_id,
